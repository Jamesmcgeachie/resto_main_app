<!DOCTYPE html>
<html>
<head>
  <title>Resto</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<h1> RESTO </h1>


<p class="navbar">
<% if current_user %>
	<%= link_to "Logout", logout_path, method: :delete %>
	<% else %>
		<%= link_to "Sign Up", new_user_path %>
		<%= link_to "Login", login_path %>
	<% end %>
	</p>

<%= yield %>

</body>
</html>
